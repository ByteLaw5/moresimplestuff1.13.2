{
    "multipart": [
        {   "apply": { "model": "moresimplestuff:block/bars_stone_post_ends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "moresimplestuff:block/bars_stone_post" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "moresimplestuff:block/bars_stone_cap" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "moresimplestuff:block/bars_stone_cap", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "moresimplestuff:block/bars_stone_cap_alt" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "moresimplestuff:block/bars_stone_cap_alt", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "moresimplestuff:block/bars_stone_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "moresimplestuff:block/bars_stone_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "moresimplestuff:block/bars_stone_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "moresimplestuff:block/bars_stone_side_alt", "y": 90 }
        }
    ]
}
